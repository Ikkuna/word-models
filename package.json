{
  "name": "word-models",
  "version": "0.0.3",
  "description": "node.js interface to the Google word2vec tool",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha",
    "postinstall": "make --directory=src",
    "test-cov": "istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "codecov": "istanbul cover ./node_modules/.bin/_mocha --dir ./reports/codecov/coverage --report lcovonly -- -R spec && cat ./reports/codecov/coverage/lcov.info | codecov && rm -rf ./reports/codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ikkuna/word-models.git"
  },
  "keywords": [
    "text-analytics",
    "deep-learning",
    "word-distance",
    "phrase-distance"
  ],
  "author": {
    "name": "Diego Correa T."
  },
  "license": "APA-2",
  "bugs": {
    "url": "https://github.com/Ikkuna/word-models/issues"
  },
  "homepage": "https://github.com/Ikkuna/word-models",
  "dependencies": {
    "SVD": "git+https://github.com/Ikkuna/SVD.git#master",
    "change-case": "^3.0.1",
    "charm": "^1.0.0",
    "mime": "^2.4.0",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "chai": "4.x.x",
    "codecov.io": "^0.1.5",
    "istanbul": "^0.4.5",
    "mocha": "5.x.x",
    "IkkunaSVD": "git+https://github.com/Ikkuna/SVD#master"
  }
}
